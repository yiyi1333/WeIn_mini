<view class="home">
    <SearchInput></SearchInput>
</view>
<view class="advertising">
    <swiper indicator-dots="true" indicator-active-color="#12a8a9" autoplay="true" circular="true">
        <swiper-item wx:for="{{swiperlist}}" wx:key="{{advertisementId}}">
            <navigator url="{{item.advertisementUrl}}" open-type="redirect"><image mode="widthFix" src="{{item.advertisementPictureUrl}}"/></navigator>
        </swiper-item>
    </swiper>
</view>