page {
  padding-bottom: 90rpx;
  background-color: #12a8a9;
}
.cart_content .cart_main {
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.cart_content .cart_main .card {
  background-color: white;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  margin-bottom: 18rpx;
  border-radius: 18rpx;
}
.cart_content .cart_main .card .deliver_address_wrap {
  padding-left: 10rpx;
}
.cart_content .cart_main .card .deliver_address_wrap .address_info {
  font-size: 28rpx;
  display: flex;
  align-items: start;
  flex-direction: column;
}
.cart_content .cart_main .card .deliver_address_wrap .address_info .address_person {
  flex: 1;
}
.cart_content .cart_main .card .deliver_address_wrap .address_info .address_phone {
  flex: 1;
}
.cart_content .cart_main .card .deliver_address_wrap .address_info .address_place {
  flex: 1;
}
.cart_content .cart_main .card .ware_address_wrap {
  padding-left: 10rpx;
}
.cart_content .cart_main .card .ware_address_wrap .address_info {
  font-size: 28rpx;
  display: flex;
  align-items: start;
  flex-direction: column;
}
.cart_content .cart_main .card .ware_address_wrap .address_info .address_person {
  flex: 1;
}
.cart_content .cart_main .card .ware_address_wrap .address_info .address_phone {
  flex: 1;
}
.cart_content .cart_main .card .ware_address_wrap .address_info .address_place {
  flex: 1;
}
.cart_content .cart_main .card .logistics_wrap {
  font-size: 38rpx;
  font-weight: 700;
  display: flex;
  /*设置显示样式**/
  align-items: center;
  /**子view垂直居中*/
  vertical-align: center;
  /**垂直居中*/
  justify-content: center;
  /**内容居中*/
  flex-direction: row;
}
.cart_content .cart_main .card .order_detail_Info {
  display: flex;
}
.cart_content .cart_main .card .order_detail_Info .title {
  padding-left: 10rpx;
  flex: 1;
  font-size: 30rpx;
  color: #73868a;
}
.cart_content .cart_main .card .order_detail_Info .info {
  flex: 3;
  font-size: 30rpx;
  font-weight: 600;
}
.cart_content .cart_main .card .shop {
  display: flex;
  padding-bottom: 10rpx;
}
.cart_content .cart_main .card .shop .shop_check_wrap {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .card .shop .shop_name {
  flex: 5;
  font-size: 34rpx;
  padding-left: 10rpx;
}
.cart_content .cart_main .card .cart_item {
  display: flex;
  padding: 10rpx;
  border-bottom: 1px solid #12a8a9;
}
.cart_content .cart_main .card .cart_item .cart_imt_wrap {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .card .cart_item .cart_imt_wrap image {
  width: 200rpx;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap {
  flex: 4;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_name {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #666;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_price_wrap {
  display: flex;
  justify-content: space-between;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_price_wrap .goods_price {
  color: #c93636;
  font-size: 34rpx;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_price_wrap .cart_bum_tool {
  display: flex;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_price_wrap .cart_bum_tool .num_edit {
  width: 66rpx;
  height: 66rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
}
.cart_content .cart_main .card .cart_item .cart_info_wrap .goods_price_wrap .cart_bum_tool .goods_num {
  width: 56rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_content .cart_main .card .order_Info {
  display: flex;
}
.cart_content .cart_main .card .order_Info .order_id_info {
  flex: 1;
  font-size: 32rpx;
  padding-left: 10rpx;
  padding-top: 16rpx;
  color: #6f8685;
}
.cart_content .cart_main .card .order_Info .order_total_price {
  flex: 1;
  padding-right: 10rpx;
  text-align: right;
}
.cart_content .cart_main .card .order_Info .order_total_price .original_price {
  font-size: 32rpx;
}
.cart_content .cart_main .card .order_Info .order_total_price .original_price .num {
  color: #c93636;
  text-decoration: line-through;
}
.cart_content .cart_main .card .order_Info .order_total_price .real_price {
  font-size: 32rpx;
}
.cart_content .cart_main .card .order_Info .order_total_price .real_price .num {
  font-size: 34rpx;
  color: #c93636;
}
.cart_content .cart_main .card .btn_wrap {
  padding-top: 16rpx;
  padding-bottom: 16rpx;
}
